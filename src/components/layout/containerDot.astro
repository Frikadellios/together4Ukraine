---
import { ShootingStars } from "@/components/react/shooting-stars";
import { StarsBackground } from "@/components/react/stars-background";

const { class: className } = Astro.props;
---

<div
  class:list={[
    "h-auto w-full h-hull rounded-tl-3xl rounded-tr-3xl -z-10 bg-trancperent",
    className,
  ]}
>
  <ShootingStars client:visible />
  <StarsBackground client:visible />
  <slot />
</div>
